<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SmallComponent" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="android:color">@color/backPrimary</item>
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">@dimen/rounding_radius</item>

    </style>

    <style name="EditText" parent="Widget.Material3.TextInputLayout.OutlinedBox">
        <item name="shapeAppearance">@style/SmallComponent</item>
        <item name="hintTextColor">?attr/colorOnSurface</item>
    </style>

    <style name="AlertDialogStyle" parent="Theme.MaterialComponents.Dialog">
        <item name="android:textColor">@color/labelPrimary</item>
        <item name="android:background">@color/backPrimary</item>
    </style>

    <style name="Body.AccentText">
        <item name="android:textColor">@color/blue</item>
    </style>


</resources>